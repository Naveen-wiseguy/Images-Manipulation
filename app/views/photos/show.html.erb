<h1> <%= @photo.title %> <h1>
<% h=@photo.height %>
<% w=@photo.width %>
<% if(@photo.maintain_aspect_ratio == true) then str="#{w}x#{h}" %>
<% else str="#{w}x#{h}#" %>
<% end %>
<%= link_to image_tag(@photo.image.thumb(str).url, alt: @photo.title), @photo.image.thumb(str).url %>
<br>
<%= button_to "Done", @photo, method: :delete %>
