 <h1>New photo upload </h1>

<%= form_for @photo do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title, required: true %>
<br>
  <%= f.label :image %>
  <%= f.file_field :image, required: true %>
<br>
  <%= f.label :height %>
  <%= f.number_field :height %>

<br>

  <%= f.label :width %>
  <%= f.number_field :width %>

<br>
  <%= f.submit 'Submit' %>

<% end %>
